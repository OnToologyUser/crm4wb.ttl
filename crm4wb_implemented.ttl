    rdfs:label "E33_E41 Linguistic Appellation"@en ;
    dcterms:description ""@en ;
    rdfs:subClassOf crm:E33_Linguistic_Object,
        crm:E41_Appellation ;
    skos:altLabel "E33_E41"@en ;
    skos:notation "E33_E41"@en ;
