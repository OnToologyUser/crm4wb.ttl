    rdfs:range lrmoo:F1_Work ;
    owl:inverseOf lrmoo:R77i_is_accompanied_or_complemented_by ;
    skos:altLabel "R77"@en ;
    skos:exactMatch crm4wb:P231 ;
    skos:notation "R77" ;
    skos:note "Wikibase ID: P231"@en,
        "Wikibase datatype: wikibase-item"@en ;
    skos:prefLabel "accompanies or complements"@en ;
    wbbs:P5 lrmoo:R77_accompanies_or_complements .

